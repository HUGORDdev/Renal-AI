{
    "devCommand": "uvicorn app.main:app --reload",
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "functions": {
    "backend/**": {                          
      "excludeFiles": "{.next,.vercel,public,app,node_modules,__pycache__,*.pyc,tests/**,docs/**,examples/**}/**"
    }
  }
}
    

